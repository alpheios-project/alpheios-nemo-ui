{
  "name": "Alpheios",
  "version": "1.0.0",
  "description": "Alpheios Reader",
  "author": "Bridget Almas",
  "private": true,
  "scripts": {
    "build": "node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all all vue",
    "build-prod": "node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all production vue",
    "build-dev": "node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all development vue",
    "lint": "eslint --fix src/**/*.js"
  },
  "dependencies": {
    "bootstrap": "^4.1.3"
  },
  "devDependencies": {
    "alpheios-components": "github:alpheios-project/components",
    "alpheios-embedded": "github:alpheios-project/embed-lib#nemoui-i182-lookup-icon",
    "alpheios-node-build": "github:alpheios-project/node-build",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    }
  }
}
